<h3>Sidebar</h3>
    <%= form_for(:user, :url => session_path(:user)) do |f| %>
      <%= f.text_field :email %><br />
      <%= f.password_field :password %><br />
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %><br />
      <%= f.submit 'Sign in' %><br />
    <% end %>
    <%= render 'devise/shared/links' %>
